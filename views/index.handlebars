{{#each articles}}
<div class="container">    
  <div class="row">
    <div>
      <div class="panel panel-primary">
        <a class="panel-heading title" href={{this.link}}>{{this.title}}</a>
        <div class="panel-body"><p>{{this.summary}}</p></div>
        <div class="panel-footer"></div>
      </div>
{{/each}}

